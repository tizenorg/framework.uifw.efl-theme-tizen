            align: 0.0 0.0;
            visible: 0;
            rel1.to: "elm.rect.title";
         }
      }
      part { name: "right_pad";
         type: RECT;
         scale : 1;
         description { state: "default" 0.0;
            min: POPUP_BASE_DEFAULT_PADDING_MIN_MAX_INC;
            max: POPUP_BASE_DEFAULT_PADDING_MIN_MAX_INC;
            fixed : 1 1;
            align: 1.0 1.0;
            visible: 0;
            rel2.to: "elm.rect.title";
