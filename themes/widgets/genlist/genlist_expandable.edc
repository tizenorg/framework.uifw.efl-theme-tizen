         program {
            name: "go_active";
            signal: "elm,state,selected";
            source: "elm";
            action: STATE_SET "selected" 0.0;
            target: "base";
            target: "elm.text.1";
            target: "elm.text.2";
         }
         program {
            name: "go_passive";
