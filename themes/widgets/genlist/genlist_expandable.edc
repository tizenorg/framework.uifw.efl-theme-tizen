            mouse_events: 0;
            scale: 1;
            description { state: "default" 0.0;
               min: 0 GENLIST_SIZE_38_INC;
               fixed: 0 1;
               align: 0.0 0.0;
               rel1 {
                  relative: 1.0 1.0;
                  to_x: "elm.swallow.pad";
