             fixed: 1 1;
            rel1 { relative: 0.5 0.5; to: "elm.rect.date.year.over"; }
            rel2 { relative: 0.5 0.5; to: "elm.rect.date.year.over"; }
         }
      }
      part {
         name: "elm.swallow.date.month";
