         description {
            state: "focus" 0.0;
            inherit: "default" 0.0;
            image.normal: "00_timepicker_field_bg_focus.png";
         }
      }
      part {
         name: "elm.image.time.min";
         type: IMAGE;
         //ignore_flags: ON_HOLD;
