            min: 0 (NAVIBAR_EX_TITLE_ITEM_H * 2);
            max: 999999 (NAVIBAR_EX_TITLE_ITEM_H * 2);
            fixed: 0 1;
            align: 0.5 0.0;
            rel1 { to: "navigationitem_separator"; }
            rel2 { to: "navigationitem_separator"; }
            visible: 1;
         }
         description { state: "closed" 0.0;
            inherit: "default" 0.0;
            visible: 0;
         }
      }
   }
   programs {
      program {
         name: "click";
         signal: "mouse,up,1";
