         source: "elm";
         action: STATE_SET "default" 0.0;
         target: "base";
         target: "elm.swallow.title";
         transition: LINEAR 0.2;
      }
      program {
         name: "retract_titlearea";
         signal: "elm,state,hide,title";
         source: "elm";
         action: STATE_SET "closed" 0.0;
         target: "base";
         target: "elm.swallow.title";
         transition: LINEAR 0.2;
      }
      program {
         name: "show_icon";
