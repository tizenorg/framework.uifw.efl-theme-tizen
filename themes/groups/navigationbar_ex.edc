         description { state: "default" 0.0;
            max: 40 40;
            rel1 { relative: 0.0 0.0; to_x: "padding_before_text";to_y: "navigationitem"; }
            rel2 { relative: 0.0 1.0; to_x: "padding_before_text";to_y: "navigationitem"; }
