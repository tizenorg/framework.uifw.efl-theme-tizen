			}\
			program {\
				name: "expand";\
				signal: "mouse,up,1";\
				source: "touch_folder";\
				action: SIGNAL_EMIT "elm,action,expand,toggle" "elm";\
			}\
			program {\
				name: "folder_down";\
				signal: "mouse,down,1";\
				source: "touch_folder";\
				script {\
					new st[31];\
