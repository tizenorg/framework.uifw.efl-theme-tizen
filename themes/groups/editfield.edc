				target: "eraser";
			}
			program { name: "editing";
				signal: "elm,state,over,hide";
				source: "elm";
				action: STATE_SET "edit" 0.0;
				target: "over";
				target: "top2";
			}
			program { name: "no_editing";
				signal: "elm,state,over,show";
				source: "elm";
				action: STATE_SET "default" 0.0;
				target: "over";
				target: "top2";
			}
			program { name: "label_show";
				signal: "elm,state,text,visible";
				source: "elm";
				action: STATE_SET "elm.text.show" 0.0;
				target: "elm.text";
				target: "top1";
				target: "bottom1";
				target: "bottom2";
				target: "bottom3";
			}
			program { name: "label_hide";
				signal: "elm,state,text,hidden";
				source: "elm";
				action: STATE_SET "default" 0.0;
				target: "elm.text";
				target: "top1";
				target: "bottom1";
				target: "bottom2";
				target: "bottom3";
			}
		}
	}

