				}
				description {
					state: "edit" 0.0;
					inherit: "default" 0.0;
					visible: 0;
				}
			}
			part { name: "eraser_image";
				scale: 1;
            			description { state: "default" 0.0;
					visible: 0;
               				min: 30 30;
					fixed: 1 1;
					align: 1 1;
               				rel1 { 
						relative: 0.0 0.0;
